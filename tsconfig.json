{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "downlevelIteration": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES6"
  },
  "include": ["src"]
}
